        "$:/plugins/amp/BT/macros/pubTitle": {
            "created": "20141231105649097",
            "creator": "Alberto Molina Pérez",
            "text": "\\define articleTitleOne()\n$(author1_surname)$ ($(year)$): “$(titre)$”\n\\end\n\n\\define articleTitleTwo()\n$(author1_surname)$ & $(author2_surname)$ ($(year)$): “$(titre)$”\n\\end\n\n\\define articleTitleThree()\n$(author1_surname)$, $(author2_surname)$ & $(author3_surname)$ ($(year)$): “$(titre)$”\n\\end\n\n\\define articleTitleFour()\n$(author1_surname)$ et al. ($(year)$): “$(titre)$”\n\\end\n\n\\define bookTitleOne()\n$(author1_surname)$ ($(year)$): $(titre)$\n\\end\n\n\\define bookTitleTwo()\n$(author1_surname)$ & $(author2_surname)$ ($(year)$): $(titre)$\n\\end\n\n\\define bookTitleThree()\n$(author1_surname)$, $(author2_surname)$ & $(author3_surname)$ ($(year)$): $(titre)$\n\\end\n\n\\define bookTitleFour()\n$(author1_surname)$ et al. ($(year)$): $(titre)$\n\\end",
