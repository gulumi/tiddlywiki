        "$:/plugins/amp/BT/forms/input/notes": {
            "created": "20141125120058912",
            "creator": "Alberto Molina Pérez",
            "text": "\\define noteTable(source,pagesChapter,title,description,about,tags,text)\n!New $(label)$\n\n$source$\n$pagesChapter$\n$title$\n$description$\n$about$\n$tags$\n$text$\n\n\\end\n\n\\define note()\n<$set name=\"tempTiddler\" value=\"$:/temp/note\" >\n<$set name=\"Label\" value=\"Note\" >\n<$set name=\"label\" value=\"note\" >\n<$set name=\"pagesChapter\" filter=\"[all[current]tag[$:/type/publication]]\" value=<<inputPagesChapter>> emptyValue=<<inputNone>> >\n\n<$reveal state=\"$(currentTab)$\" type=\"nomatch\" text=\"\">\n\n<$macrocall $name=\"noteTable\" \n    source=<<inputSource>>\n    pagesChapter=<<pagesChapter>>\n    title=<<inputTitle>>\n    description=<<inputDescription>>\n    about=<<inputAbout>>\n    tags=<<inputTags>>\n    text=<<inputText>>\n    />\n\n</$reveal>\n\n<$reveal state=\"$(currentTab)$\" type=\"match\" text=\"\">\n\n<$macrocall $name=\"noteTable\" \n    source=\"\"\n    pagesChapter=\"\"\n    title=<<inputTitle>>\n    description=<<inputDescription>>\n    about=<<inputAbout>>\n    tags=<<inputTags>>\n    text=<<inputText>>\n    />\n\n</$reveal>\n\n</$set>\n</$set>\n</$set>\n</$set>\n\\end\n\n\n\n\n\n\n\n\n\n\n<<note>>",
            "modified": "20141215132612291",
