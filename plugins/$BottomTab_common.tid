        "$:/plugins/amp/BT/macros/newNote/button": {
            "created": "20141125101553189",
            "creator": "Alberto Molina Pérez",
            "text": "\\define newNoteButton2(type, title, source, tags, label, class)\n<$button class=\"$class$\">\n    <$action-sendmessage $message=\"tm-new-tiddler\" $param=\"$:/temp/$type$\"\n        title={{$title$}}\n        tags=\"$:/type/$type$ $tags$\"\n        icon={{$:/type/$type$!!icon}}\n        color={{$:/type/$type$!!color}}\n        source=\"[[$source$]]\"\n        about={{$:/temp/$type$!!about}}\n        description={{$:/temp/$type$!!description}}\n        />\n    <$action-deletefield $tiddler=\"$:/temp/$type$\" tags text about description chapter page_alpha page_omega/>\n    <$action-deletefield $tiddler=\"$:/temp/title\" text/>\n    $label$\n</$button>\n\\end\n\n\\define newNoteButton(type:\"note\", title:\"$:/temp/title\", tags, label:\"add\", class:\"\")\n<$reveal state=\"$title$\" type=\"nomatch\" text=\"\">\n<$reveal state=\"$(currentTab)$\" type=\"nomatch\" text=\"\">\n\n    <$macrocall $name=\"newNoteButton2\"\n        type=\"$type$\"\n        title=\"$title$\"\n        tags=\"$tags$\"\n        source={{!!title}}\n        label=\"$label$\"\n        class=\"$class$\"\n        />\n\n</$reveal>\n\n<$reveal state=\"$(currentTab)$\" type=\"match\" text=\"\">\n\n    <$macrocall $name=\"newNoteButton2\"\n        type=\"$type$\"\n        title=\"$title$\"\n        tags=\"$tags$\"\n        label=\"$label$\"\n        class=\"$class$\"\n        />\n</$reveal>\n</$reveal>\n\\end\n\n<<newNoteButton>>",
            "modified": "20141231104217984",
            "modifier": "Alberto Molina Pérez",
            "tags": "BottomTabs $:/tags/Macro",
            "title": "$:/plugins/amp/BT/macros/newNote/button"
