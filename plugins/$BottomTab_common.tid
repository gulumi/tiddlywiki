        "$:/plugins/amp/BT/css/buttons": {
            "created": "20141213163744005",
            "creator": "Alberto Molina Pérez",
            "text": ".agenda td button {\n\tpadding: 10%;\n\tmargin: 0px 0px;\n\twidth: 100%;\n\theight: 100%;\n\tborder: none;\n\ttext-align: center;\n\tbackground: #FFFFDD;\n}\n\n.btn-touch-big {\n\tmargin: 0px 12px 0px 12px;\n\tbackground: none;\n\tcolor: #999999;\n\tborder: none;\n\tfont-size: 4em;\n\tline-height: 1em;\n}\n\n.btn-touch-big-down {\n\tmargin: 0px 12px 0px 12px;\n\tbackground: none;\n\tcolor: #333333;\n\tborder: none;\n\tfont-size: 4em;\n\tline-height: 1em;\n}\n\n.btn-touch-big:hover {\n\tcolor: <<couleur btn-courses-hover>>;\n}\n\n.btn-sidebar {\n\theight: 6em;\n\twidth: 6em;\n\tmargin: 0px -10px 0px 0px;\n\tbackground: none;\n\tcolor: <<color sidebar-controls-foreground>>;\n\tborder: none;\n\tfont-size: 1em;\n\tline-height: 1.2em;\n}\n\n.btn-sidebar-down {\n\theight: 6em;\n\twidth: 6em;\n\tmargin: 0px -10px 0px 0px;\n\tbackground: none;\n\tcolor: <<color sidebar-controls-foreground-hover>>;\n\tborder: none;\n\tfont-size: 1em;\n\tline-height: 1.4em;\n}\n\n.btn-sidebar:hover {\n\tcolor: <<color sidebar-controls-foreground-hover>>;\n}\n\n.btn-sidebar-down:hover {\n\tcolor: <<color sidebar-controls-foreground>>;\n}\n\n.btn-newHere-sidebar {\n\tbackground: none;\n\tcolor: #FFFFFF;\n\tborder: 1px solid white;\n\tfont-size: 1.2em;\n\tfont-weight:bold;\n\tline-height: 1.6em;\n}\n\n\n.btn-big {\n\tpadding: 8px;\n\tmargin: 0px 8px 4px 0px;\n\tcolor: <<colour download-foreground>>;\n\tfill: <<colour download-foreground>>;\n\tborder: none;\n\tfont-size: 1.2em;\n\tfont-weight: bold;\n\tline-height: 1.4em;\n}\n\n.btn-big.grey {\n\tbackground: grey;\n}\n\n.btn-big.green {\n\tbackground: <<colour download-background>>;\n}\n\n.btn-big.red {\n\tbackground: red;\n}\n\n.btn-inputSlider {\n\tpadding: 0;\n\tmargin: 0;\n\tbackground: none;\n\tborder: none;\n}\n\n.btn-inputSlider.tab {\n\tfloat: right;\n\tposition:relative;\n\ttop: 6px;\n}\n\n.btn-inputSlider.home {\n\tfloat: right;\n\tposition:relative;\n\ttop: 30px;\n}\n\n",
            "modified": "20141231101108121",
            "modifier": "Alberto Molina Pérez",
            "tags": "$:/tags/Stylesheet BottomTabs",
            "title": "$:/plugins/amp/BT/css/buttons"
        },
