        "$:/plugins/amp/BT/config/BottomTabs": {
            "created": "20140720092609150",
            "creator": "Alberto Molina Pérez",
            "text": "!Default tabs\n\n<$checkbox tiddler=\"$:/plugins/amp/BT/tabs/info\" tag=\"$:/action/is/default\"> \n    <$link>\n        <$view tiddler=\"$:/plugins/amp/BT/tabs/info\" field=\"description\"/>\n    </$link>\n</$checkbox>   \n<$checkbox tiddler=\"$:/plugins/amp/BT/tabs/comments\" tag=\"$:/action/is/default\"> \n    <$link>\n        <$view tiddler=\"$:/plugins/amp/BT/tabs/comments\" field=\"description\"/>\n    </$link>\n</$checkbox>\n\n<div class=\"troiscolonnes\">\n<$list filter=\"[all[tiddlers+shadows]tag[$:/type/tab]] -[[$:/plugins/amp/BT/tabs/info]] -[[$:/plugins/amp/BT/tabs/comments]] +[sort[title]]\">\n    <$checkbox tag=\"$:/action/is/default\"> \n        <$link>\n            <$view field=\"description\"/>\n        </$link>\n    </$checkbox>\n<br>\n\n</$list>\n</div>\n\n!Position\n\n<$radio tiddler=\"$:/config/BottomTabs/position/$:/plugins/amp/BT\" field=\"text\" value=\"left\">Left</$radio>      \n<$radio tiddler=\"$:/config/BottomTabs/position/$:/plugins/amp/BT\" field=\"text\" value=\"right\">Right</$radio>\n\n! Colors\n\n<$tiddler tiddler=\"$:/config/BottomTabs/colors/$:/plugins/amp/BT\">\n\n|!Blue: |<$edit-text index=\"blue\" tag=\"input\"/>|<$edit-text index=\"blue\" type=\"color\" tag=\"input\"/>|\n|!Red: |<$edit-text index=\"red\" tag=\"input\"/>|<$edit-text index=\"red\" type=\"color\" tag=\"input\"/>|\n|!Yellow: |<$edit-text index=\"yellow\" tag=\"input\"/>|<$edit-text index=\"yellow\" type=\"color\" tag=\"input\"/>|\n\n</$tiddler>\n\n!Caption\n\nFont-size: <$edit-text tiddler=\"$:/config/BottomTabs/caption/font-size/$:/plugins/amp/BT\" default=\"1em\" tag=\"input\" size=\"6\"/>\n\nFont-size selected: <$edit-text tiddler=\"$:/config/BottomTabs/caption-selected/font-size/$:/plugins/amp/BT\" tag=\"input\" size=\"6\"/>\n\nFont-size counter: <$edit-text tiddler=\"$:/config/BottomTabs/caption-counter/font-size/$:/plugins/amp/BT\" tag=\"input\" size=\"6\"/>\n\nColor: <$edit-text tiddler=\"$:/config/BottomTabs/caption/color/$:/plugins/amp/BT\" tag=\"input\" default=\"\"/> <$edit-text tiddler=\"$:/config/BottomTabs/caption/color/$:/plugins/amp/BT\" tag=\"input\" type=\"color\"/>\n\nCounter: \n<$radio tiddler=\"$:/config/BottomTabs/caption/counter/$:/plugins/amp/BT\" field=\"text\" value=\"show\">Show</$radio>      \n<$radio tiddler=\"$:/config/BottomTabs/caption/counter/$:/plugins/amp/BT\" field=\"text\" value=\"hide\">Hide</$radio>",
            "caption": "Tabs bar",
