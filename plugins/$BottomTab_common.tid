            "tags": "BottomTabs $:/type/config",
            "title": "$:/plugins/amp/BT/config/BottomTabs",
            "dependencies": "$:/plugins/amp/BT/tabs/info $:/plugins/amp/BT/tabs/comments $:/config/BottomTabs/position/$:/plugins/amp/BT $:/config/BottomTabs/colors/$:/plugins/amp/BT"
